{
	"name": "nativescript-audioplayer",
	"version": "0.0.2",
	"description": "Fully featured AudioPlayer NativeScript plugin.",
	"nativescript": {
		"platforms": {
			"android": "2.0.0",
			"ios": "2.0.0"
		}
	},
	"main": "audioplayer.js",
	"scripts": {
		"build": "tsc",
		"demo.ios": "npm run preparedemo && cd demo && tns emulate ios",
		"demo.android": "npm run preparedemo && cd demo && tns run android",
		"preparedemo": "npm run build && npm run reloadplugin",
		"reloadplugin": "cd demo && tns plugin remove nativescript-audioplayer && tns plugin add .. && tns install",
		"apk": "cd demo && tns build android --copy-to ."
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/dfg-nota/nativescript-audioplayer-plugin.git"
	},
	"keywords": [
		"NativeScript",
		"JavaScript",
		"Android",
		"iOS"
	],
	"author": {
		"name": "Daniel Freiling",
		"email": "dfg@nota.dk"
	},
	"bugs": {
		"url": "https://github.com/dfg-nota/nativescript-audioplayer-plugin/issues"
	},
	"homepage": "https://github.com/dfg-nota/nativescript-audioplayer-plugin",
	"readmeFilename": "README.md",
	"devDependencies": {
		"typescript": "^1.8.10"
	}
}