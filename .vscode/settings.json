{
    "files.exclude": {
        "*.d.ts": true,
        "*.js": true,
        "**/.DS_Store": true,
        "**/.git": true,
        "**/*.js": {
            "when": "$(basename).ts"
        },
        "**/*.map": true,
        "demo/platforms/**": true
    },
    "search.exclude": {
        "**/*.code-search": true,
        "**/bower_components": true,
        "**/node_modules": true,
        "demo/platforms/**": true
    },
    "editor.tabSize": 2,
    "editor.insertSpaces": true,
    "cSpell.words": [
        "Astley",
        "Freiling",
        "NSURL",
        "Prebuffer",
        "Siri",
        "androidx",
        "audioplayer",
        "downlevel",
        "exoplayer",
        "justlaunch",
        "nospace",
        "prettierrc",
        "quotemark",
        "trackselection",
        "wifi"
    ],
    "typescript.tsdk": "node_modules/typescript/lib",
    "files.watcherExclude": {
        "demo/platforms/**": true
    }
}
