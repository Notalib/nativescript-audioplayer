{
    "files.exclude": {
        "*.d.ts": true,
        "*.js": true,
        "**/.DS_Store": true,
        "**/.git": true,
        "**/*.js": {
            "when": "$(basename).ts"
        },
        "**/*.map": true,
        "demo/platforms/**": true
