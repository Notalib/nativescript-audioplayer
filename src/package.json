{
  "name": "@nota/nativescript-audioplayer",
  "version": "8.4.2",
  "license": "LGPL-3.0",
  "description": "Fully featured AudioPlayer for NativeScript. Supports gap-less audio playlist playback with variable playback speed and native remote controls",
  "nativescript": {
    "platforms": {
      "android": "8.5.0",
      "ios": "8.5.0"
    }
  },
  "main": "index",
  "typings": "index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/Notalib/nativescript-audioplayer.git"
  },
  "keywords": [
    "NativeScript",
    "JavaScript",
    "Android",
    "iOS"
  ],
  "author": {
    "name": "Daniel Freiling",
    "email": "dfg@nota.dk"
  },
  "contributors": [
    {
      "name": "Morten Sj√∏gren",
      "email": "m_abs@mabs.dk",
      "url": "https://mabs.dk"
    }
  ],
  "bugs": {
    "url": "https://github.com/Notalib/nativescript-audioplayer/issues"
  },
  "homepage": "https://github.com/Notalib/nativescript-audioplayer",
  "readmeFilename": "README.md",
  "scripts": {
    "postinstall": "ts-patch install",
    "format:check": "prettier -c --config $(git rev-parse --show-toplevel)/.prettierrc.json './**/*.ts'",
    "tslint": "tslint './**/*.ts' --config $(git rev-parse --show-toplevel)/tslint.json -p tsconfig.json --exclude \"**/node_modules/**\"",
    "lint": "npm-run-all format:check tslint",
    "build": "tsc"
  },
  "devDependencies": {
    "@nativescript/core": "^8.4.7",
    "@nativescript/types": "^8.5.0",
    "@nativescript/webpack": "^5.0.14",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.7",
    "ts-patch": "^2.1.0",
    "tslint": "~6.1.3",
    "typescript": "^4.9.5"
  }
}
